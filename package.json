{
  "name": "baby-skill-tree",
  "version": "0.1.0",
  "description": "A D3 graph of an \"average\" baby's developmental milestones.",
  "author": "Trygve Wastvedt <trygve.wastvedt@gmail.com>",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "@types/d3": "^7.0.0",
    "@types/d3-scale": "^4.0.1",
    "@types/d3-zoom": "^3.0.1",
    "@types/node": "^16.4.13",
    "cpx": "^1.5.0",
    "d3": "7.x",
    "d3-dsv": "^3.0.1",
    "d3-scale": "^4.0.0",
    "d3-zoom": "^3.0.0",
    "parcel": "^2.0.0-rc.0",
    "sass": "^1.37.5",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "copyIcons": "cpx src/icons/* dist/icons --clean",
    "start": "concurrently \"parcel ./src/index.html\" \"npm run copyIcons -- --watch\"",
    "build": "npm run copyIcons && parcel build ./src/index.html --public-url ./",
    "prettier": "node ./node_modules/prettier/bin-prettier.js --write ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "npm:@btmills/prettier@^2.3.1"
  }
}
